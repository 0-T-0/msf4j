        </dependency>

        <dependency>
            <groupId>org.wso2.carbon.mss</groupId>
            <artifactId>petstore-util</artifactId>
            <version>1.0.0-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>com.google.guava</groupId>
            <artifactId>guava</artifactId>
            <version>13.0.1</version>
        </dependency>
